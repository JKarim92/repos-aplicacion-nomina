@model aplicacionNomina.Core.Models.Title
@{
    ViewData["Title"] = "Nuevo título";
}
<h3>Nuevo título</h3>
<form asp-action="Create" method="post" class="row g-3">
  <input type="hidden" asp-for="EmpNo"/>
  <div class="col-md-6">
    <label class="form-label">Título</label>
    <input asp-for="TitleName" class="form-control"/>
    <span asp-validation-for="TitleName" class="text-danger"></span>
  </div>
  <div class="col-md-3">
    <label class="form-label">Desde</label>
    <input asp-for="FromDate" type="date" class="form-control"/>
    <span asp-validation-for="FromDate" class="text-danger"></span>
  </div>
  <div class="col-12">
    <button class="btn btn-primary">Guardar</button>
  </div>
</form>
@section Scripts{<partial name="_ValidationScriptsPartial"/>}
